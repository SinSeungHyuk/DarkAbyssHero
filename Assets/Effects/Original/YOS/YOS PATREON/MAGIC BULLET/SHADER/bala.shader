				float2 uv_mask = IN.ase_texcoord3.xy * _mask_ST.xy + _mask_ST.zw;
				float4 appendResult22 = (float4(_noisevelxy.x , _noisevelxy.y , 0.0 , 0.0));
				float2 uv_noise = IN.ase_texcoord3.xy * _noise_ST.xy + _noise_ST.zw;
				float2 panner18 = ( 1.0 * _Time.y * appendResult22.xy + uv_noise);
				float temp_output_9_0 = ( pow( tex2D( _mask, uv_mask ).r , _powermask ) * pow( (_clampminnoise + (tex2D( _noise, panner18 ).r - 0.0) * (1.0 - _clampminnoise) / (1.0 - 0.0)) , _powernoise ) );
				float clampResult11 = clamp( ( temp_output_9_0 * _opac ) , 0.0 , 1.0 );
				float smoothstepResult32 = smoothstep( 0.05 , 1.0 , clampResult11);
				float ifLocalVar34 = 0;
				if( smoothstepResult32 > _DESTRUCTOROPACIDAD1 )
