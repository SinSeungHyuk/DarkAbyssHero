						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 uv_texturamask = IN.ase_texcoord3.xy * _texturamask_ST.xy + _texturamask_ST.zw;
				float4 appendResult28 = (float4(_vely , 0.0 , 0.0 , 0.0));
				float2 uv_noise = IN.ase_texcoord3.xy * _noise_ST.xy + _noise_ST.zw;
				float2 panner14 = ( 1.0 * _Time.y * appendResult28.xy + uv_noise);
				float4 texCoord17 = IN.ase_texcoord3;
				texCoord17.xy = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float temp_output_19_0 = ( _destructor * ( ( tex2D( _texturamask, uv_texturamask ).r * pow( tex2D( _noise, (panner14*1.0 + texCoord17.z) ).r , _powertexture ) ) * IN.ase_color.a ) );
				float temp_output_21_0 = ( floor( temp_output_19_0 ) / _destructor );
				float2 temp_cast_1 = (pow( temp_output_21_0 , _powercolor )).xx;
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( tex2D( _grad, temp_cast_1 ) * _Color0 ).rgb;
				float Alpha = ( temp_output_21_0 * _opac );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

